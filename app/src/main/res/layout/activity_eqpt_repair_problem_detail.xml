<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F1F1F1">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:background="@color/colorAccent">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="45dp"
                    android:text="工单信息"
                    android:textColor="#FFFFFF"
                    android:textSize="15sp"
                    android:gravity="center"/>
            </RelativeLayout>
        </LinearLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="39dp"
        android:background="#FFFFFF"
        android:paddingLeft="11dp"
        android:paddingRight="11dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:text="工单任务"
            android:textColor="#333333"
            android:textSize="13sp" />

        <TextView
            android:id="@+id/eqpt_tepair_task"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:textColor="#888888"
            android:textSize="13sp" />
    </RelativeLayout>

    <TextView
            android:layout_width="match_parent"
            android:layout_height="3dp"
            android:background="#D9D9D9"/>

        <RelativeLayout
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:paddingLeft="11dp"
            android:paddingRight="11dp"
            android:background="#FFFFFF">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="任务描述"
                android:textColor="#333333"
                android:textSize="13sp"
                android:layout_centerVertical="true"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#888888"
                android:textSize="13sp"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:id="@+id/eqpt_tepair_task_des"/>
        </RelativeLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:background="#D9D9D9"/>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:paddingLeft="11dp"
            android:paddingRight="11dp"
            android:background="#FFFFFF">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="班组"
                android:textColor="#333333"
                android:textSize="13sp"
                android:layout_centerVertical="true"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#888888"
                android:textSize="13sp"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:id="@+id/ovepargroupname"/>
        </RelativeLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="3dp"
            android:background="#D9D9D9"/>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:paddingLeft="11dp"
            android:paddingRight="11dp"
            android:background="#FFFFFF">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="二次派工时间"
                android:textColor="#333333"
                android:textSize="13sp"
                android:layout_centerVertical="true"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#888888"
                android:textSize="13sp"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:id="@+id/dispaordercontroldispatime"/>
        </RelativeLayout>



        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:paddingLeft="11dp"
            android:paddingRight="11dp"
            android:background="#FFFFFF">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="完工时间"
                android:textColor="#333333"
                android:textSize="13sp"
                android:layout_centerVertical="true"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#888888"
                android:textSize="13sp"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:id="@+id/completordersubmittime"/>
        </RelativeLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="3dp"
        android:background="#D9D9D9"/>


    <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:paddingLeft="11dp"
            android:paddingRight="11dp"
            android:background="#FFFFFF">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="问题现象"
                android:textColor="#333333"
                android:textSize="13sp"
                android:layout_centerVertical="true"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#888888"
                android:textSize="13sp"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:id="@+id/problemphenomenonname"/>
        </RelativeLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="3dp"
            android:background="#D9D9D9"/>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:paddingLeft="11dp"
            android:paddingRight="11dp"
            android:background="#FFFFFF">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="问题现场处理方式"
                android:textColor="#333333"
                android:textSize="13sp"
                android:layout_centerVertical="true"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#888888"
                android:textSize="13sp"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:id="@+id/inspproblemscenetreatmentmodeentry"/>
        </RelativeLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="3dp"
        android:background="#D9D9D9"/>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:paddingLeft="11dp"
        android:paddingRight="11dp"
        android:background="#FFFFFF">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="故障原因"
            android:textColor="#333333"
            android:textSize="13sp"
            android:layout_centerVertical="true"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#888888"
            android:textSize="13sp"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:id="@+id/faultcausecontent"/>
    </RelativeLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="3dp"
        android:background="#D9D9D9"/>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:paddingLeft="11dp"
        android:paddingRight="11dp"
        android:background="#FFFFFF">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="故障机理"
            android:textColor="#333333"
            android:textSize="13sp"
            android:layout_centerVertical="true"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#888888"
            android:textSize="13sp"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:id="@+id/faultmechanismcontent"/>
    </RelativeLayout>

    <TextView

        android:layout_width="match_parent"
        android:layout_height="3dp"
        android:background="#D9D9D9"/>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:paddingLeft="11dp"
        android:paddingRight="11dp"
        android:background="#FFFFFF">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="故障处理措施"
            android:textColor="#333333"
            android:textSize="13sp"
            android:layout_centerVertical="true"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#888888"
            android:textSize="13sp"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:id="@+id/faulttreatmentmeascontent"/>
    </RelativeLayout>

    <TextView

        android:layout_width="match_parent"
        android:layout_height="3dp"
        android:background="#D9D9D9"/>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:paddingLeft="11dp"
        android:paddingRight="11dp"
        android:background="#FFFFFF">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="问题状态"
            android:textColor="#333333"
            android:textSize="13sp"
            android:layout_centerVertical="true"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#888888"
            android:textSize="13sp"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:id="@+id/inspproblemstatename"/>
    </RelativeLayout>

    <TextView

        android:layout_width="match_parent"
        android:layout_height="3dp"
        android:background="#D9D9D9"/>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:paddingLeft="11dp"
        android:paddingRight="11dp"
        android:background="#FFFFFF">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="派工人员"
            android:textColor="#333333"
            android:textSize="13sp"
            android:layout_centerVertical="true"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#888888"
            android:textSize="13sp"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:id="@+id/vi_dispa_staff"/>
    </RelativeLayout>
</LinearLayout>